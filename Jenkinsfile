pipeline{
    agent any
        stages{
            stage('checkout code'){
                steps{
                    echo " this is my first pipeline job"
            }
            stage('build') {
                steps{
                    echo "this is my 2nd pipeline job"
                }
            }
            stage('any_name'){
                steps{
                    echo "this is my 3rd pipeline job"
                }
            }
        }
    }